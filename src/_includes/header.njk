<header>
  <nav>
    <ul>
      {% for link in site.mainNavLinks %}
        <li>
          <a href="{{ link.url }}"
            {% if page.url == link.url %}
              aria-current="page"
            {% endif %}>{{ link.text }}
          </a>
        </li>
      {% endfor %}
    </ul>
  </nav>
  <a href='#' class="stt" title="scroll to top"></a>
</header>
